{
  "files.watcherExclude": {
    "**/target": true
  },
  "prettier.printWidth": 90,
  "cSpell.words": [
    "bbox",
    "DAST",
    "dsantini",
    "Geobuf",
    "gitlab",
    "iiif",
    "imposm",
    "mapbox",
    "mapcomplete",
    "Maplibre",
    "maplibregl",
    "openstreetmap",
    "osmdata",
    "owmf",
    "panoramax",
    "pmtiles",
    "qlever",
    "rdfs",
    "SAST",
    "skel",
    "sourcedata",
    "sparql",
    "WDQS",
    "wikibase",
    "wikilink",
    "wikispore",
    "workdir"
  ],
  "python.linting.pylint": true,
  "python.formatting.autopep8": true,
  "dotenv.enableAutocloaking": false,
  "i18n-ally.localesPaths": [
    "public/locales"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
  },
  "i18n-ally.keystyle": "nested",
  "i18n-ally.indent": 4,
  "jest.runMode": "on-demand",
  "jest.rootPath": "front-end"
}
