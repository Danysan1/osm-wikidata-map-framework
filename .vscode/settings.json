{
  "files.watcherExclude": {
    "**/target": true
  },
  "prettier.printWidth": 90,
  "cSpell.words": [
    "bbox",
    "CURLOPT",
    "DAST",
    "dsantini",
    "Geobuf",
    "gitlab",
    "imposm",
    "mapboxgl",
    "Maplibre",
    "maplibregl",
    "openstreetmap",
    "osmdata",
    "owmf",
    "panoramax",
    "qlever",
    "rdfs",
    "RETURNTRANSFER",
    "SAST",
    "skel",
    "sourcedata",
    "sparql",
    "VERIFYHOST",
    "VERIFYPEER",
    "WDQS",
    "wikibase",
    "workdir"
  ],
  "python.linting.pylint": true,
  "python.formatting.autopep8": true,
  "dotenv.enableAutocloaking": false,
  "i18n-ally.localesPaths": [
    "public/locales"
  ],
  "[typescript]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
  },
  "i18n-ally.keystyle": "nested",
  "i18n-ally.indent": 4
}
