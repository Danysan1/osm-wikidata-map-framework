{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.7.0",
    "generators": {
      "commons": {
        "inputSpec": "./commons.yaml",
        "output": "../front-end/src/generated/commons",
        "generatorName": "typescript-fetch",
        "additionalProperties": {
          "modelPropertyNaming": "original"
        }
      },
      "sparql": {
        "glob": "./sparql.yaml",
        "output": "../front-end/src/generated/sparql",
        "generatorName": "typescript-fetch",
        "additionalProperties": {
          "modelPropertyNaming": "original"
        }
      }
    }
  }
}