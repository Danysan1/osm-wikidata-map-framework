####################################
####                            ####
####        Data source         ####
####                            ####
####################################

# osm_filter_tags: Optional (required if you initialize the DB from OSM PBF dump), OSM tags to use for filtering the results
osm_filter_tags=["name=*","historic=tomb","cemetery=grave"]
# osm_text_key: Optional, OSM key whose value contains the name of the etymology
osm_text_key=name:etymology
# osm_description_key: Optional, OSM key whose value contains the description of the etymology
osm_description_key=name:etymology:description
# osm_wikidata_keys: Optional, OSM keys which connect the etymology subject (map element) to the etymology object (popup element)
osm_wikidata_keys=["name:etymology:wikidata","subject:wikidata","buried:wikidata"]
# osm_wikidata_properties: Optional, Wikidata properties which connect the etymology subject (map element) to the etymology object (popup element)
osm_wikidata_properties=["P138","P825","P547"]
# propagate_data: Optional ("global", "local" or false, default: false), whether etymologies should be propagated
propagate_data=global
# wikidata_indirect_property: Optional, Wikidata property which connects the etymology object (popup element) with a relation whose P625 qualifier identifies the coordinates of the etymology subject (map element)
wikidata_indirect_property=P119
# wikidata_image_property: Optional, Wikidata property which points to a Wikimedia Commons image of the etymology subject (map element)
wikidata_image_property=P1442

####################################
####                            ####
####         Front-end          ####
####                            ####
####################################

##### Logs #####
# #log_file_path: Optional, path to back-end log file
#log_file_path=
# enable_debug_log: Optional, enables detailed front-end logs
enable_debug_log=true

##### Map background #####
# mapbox_token: Optional, Mapbox API access token
#   See https://docs.mapbox.com/help/getting-started/access-tokens/
#   Create your token at https://account.mapbox.com/access-tokens/
mapbox_token=
# maptiler_key: Optional, Maptiler API key
#   See https://docs.maptiler.com/cloud/api/authentication-key/
#   Create your key at https://cloud.maptiler.com/account/keys/
maptiler_key=
# enable_stadia_maps: Optional, switch to enable Stadia maps vector tiles
#   See https://docs.stadiamaps.com/authentication/
#   If you access your OWMF instance from http://localhost or http://127.0.0.1 it should work out of the box
#   Before enabling in a deployed instance, create a property for your domain at https://client.stadiamaps.com/dashboard/#/property/create
enable_stadia_maps=true
# maptiler_key: Optional, Jawg maps API key
#   See https://www.jawg.io/docs/apidocs/maps/dynamic-maps/#endpoint-to-use
#   Create your token at https://www.jawg.io/lab/access-tokens
jawg_token=

##### Map behavior #####
# elements_bbox_max_area: Required >0, maximum area of bounding box for elements queries, in square degrees
elements_bbox_max_area=2.3
# wikidata_bbox_max_area: Required >0, maximum area of bounding box for etymology queries, in square degrees
wikidata_bbox_max_area=0.012
# threshold_zoom_level: Required >0, zoom level at which to switch from clustered result to individual elements
threshold_zoom_level=13
# min_zoom_level: Required >0, minimum zoom level to fetch any data (below this zoom the global clustered data will be shown)
min_zoom_level=9
# bbox_margin: Optional >=0, margin to add to bounding box before fetching data, in degrees
bbox_margin=0.002
# show_feature_picture: Optional, whether to show the picture on the feature details popup
show_feature_picture=true
# show_feature_mapcomplete: Optional, whether to show the MapComplete button on the feature details popup
show_feature_mapcomplete=true

##### Map defaults #####
# default_center_lat: Required, default latitude for map center
default_center_lat=45
# default_center_lon: Required, default longitude for map center
default_center_lon=15
# default_zoom: Required >0, default zoom level for map center
default_zoom=3
# default_language: Required, ISO 639-1 language code for the default language (https://www.fincher.org/Utilities/CountryLanguageList.shtml)
default_language=en
# default_background_style: Required, default background style for the map
default_background_style=stadia_osm_bright
# default_color_scheme: Required, default color scheme for the map
default_color_scheme=blue
# default_source: Required, default source of the map
default_source=overpass_all

##### Web infobox and taginfo.json #####
# i18n_override: Required, i18n strings to overwrite the default strings in public/locales/*/translation.json
# Must be in a JSON format supported by i18next-resources-to-backend, with "app" as namespace
# See https://github.com/i18next/i18next-resources-to-backend
i18n_override='{"en":{"app":{"title":"Project title","description":"Project description"}},"it":{"app":{"title":"Titolo del progetto","description":"Descrizione del progetto"}}}'
# home_url: Required, homepage of the project
home_url=https://gitlab.com/openetymologymap/osm-wikidata-map-framework
# issues_url: Required, link to the issue tracker
issues_url=https://gitlab.com/openetymologymap/osm-wikidata-map-framework/-/issues
# contributing_url: Required, link to a documentation about how to contribute to the data
contributing_url=https://gitlab.com/openetymologymap/osm-wikidata-map-framework/-/blob/main/CONTRIBUTING.md#how-to-contribute-to-the-etymology-data
# element_issue_url: Required, link to a documentation about how to fix a wrong item
element_issue_url=https://gitlab.com/openetymologymap/osm-wikidata-map-framework/-/blob/main/CONTRIBUTING.md#how-to-report-a-problem-in-the-etymology-of-an-element
# propagation_docs_url: Required, link to a documentation about how data is propagated
propagation_docs_url=https://gitlab.com/openetymologymap/osm-wikidata-map-framework/-/blob/main/CONTRIBUTING.md#propagation
# keywords: Optional, keywords for the project
keywords='OpenStreetMap,Wikidata,map,mappa,karte,open data,linked data,structured data,street,place,poi,point of interest'
# contact_name: Required for taginfo.json, name of the person responsible for the app
contact_name=
# contact_email: Required for taginfo.json, email of the person responsible for the app
contact_email=

##### Sentry JS (client-side) #####
# sentry_js_dsn: Optional, Sentry JS DSN ( https://docs.sentry.io/platforms/javascript/ )
sentry_js_dsn=
# sentry_js_env: Optional, Sentry JS environment
sentry_js_env=
# sentry_js_uri: Optional, Sentry JS Content Security Policy report URI ( https://docs.sentry.io/product/security-policy-reporting/ )
sentry_js_uri=
# sentry_js_replays_session_sample_rate: Optional, Sentry JS session replay sample rate ( https://docs.sentry.io/platforms/javascript/session-replay/ )
sentry_js_replays_session_sample_rate=0
# sentry_js_replays_on_error_sample_rate: Optional, Sentry JS session replay sample rate
sentry_js_replays_on_error_sample_rate=1

##### Statistics #####
# google_analytics_id: Optional, Google Analytics GTAG ID
google_analytics_id=
# matomo_domain: Optional, Matomo instance URL
matomo_domain=
# matomo_id: Optional, Matomo site ID
matomo_id=

##### PayPal donation button #####
# paypal_id: Optional, PayPal business ID for the donation button
paypal_id=NA5HL6EM9LDJ6

####################################
####                            ####
####          Back-end          ####
####                            ####
####################################

##### Wikidata API options #####
# wikidata_endpoint: Required, URL of Wikidata SPARQL endpoint (https://www.wikidata.org/wiki/Wikidata:SPARQL_query_service#Interfacing)
wikidata_endpoint=https://query.wikidata.org
# wikidata_cache_timeout_hours: Required, max Wikidata cache age
wikidata_cache_timeout_hours=2
# eager_full_etymology_download: Optional, enables eager downlod of etymology details on back-end instead of lazy-loading on front-end
eager_full_etymology_download=false
# max_map_elements: Optional >0, maximum number of map elements to fetch beyond the threshold zoom level (applies to all sources)
max_map_elements=
# max_wikidata_elements: Optional >0, maximum number of wikidata IDs to fetch details and stats about
max_wikidata_elements=

##### Overpass API options #####
# overpass_endpoints: Required, URL of Overpass API endpoints (https://wiki.openstreetmap.org/wiki/Overpass_API#Public_Overpass_API_instances)
overpass_endpoints=["https://overpass-api.de/api/interpreter","https://maps.mail.ru/osm/tools/overpass/api/interpreter","https://overpass.openstreetmap.ru/api/interpreter","https://overpass.kumi.systems/api/interpreter"]
# cache_file_base_path: Required, base path for cache files
cache_file_base_path=/var/www/html/map-cache/
# cache_file_base_url: Required, base path for cache files
cache_file_base_url=map-cache/
# overpass_cache_timeout_hours: Required, max Overpass cache age
overpass_cache_timeout_hours=2
# fetch_nodes: Optional, true to fetch nodes (used only when using Overpass)
fetch_nodes=true
# fetch_ways: Optional, true to fetch ways (used only when using Overpass)
fetch_ways=true
# fetch_relations: Optional, true to fetch relations (used only when using Overpass)
fetch_relations=false

##### Database connection options #####
# db_enable: Optional, enable/disable usage of a PostgreSQL+PostGIS database (initialized with db-init) instead of using Overpass
db_enable=false
# db_host: Optional, host of the DBMS
db_host=owmf-postgis
# db_port: Optional, port of the DBMS
db_port=5432
# db_user: Optional, user of the DBMS
db_user=owmf
# db_password: Optional, password of the DBMS
db_password=_CHANGE_ME_
# db_database: Optional, database to use in the DBMS
db_database=owmf
# db_sslmode: Optional, whether to use SSL when connecting to the DBMS (see https://www.postgresql.org/docs/15/libpq-ssl.html#LIBPQ-SSL-PROTECTION )
db_sslmode=prefer
# db_endpoint: Optional, endpoint connection option, if necessary (see https://neon.tech/docs/connect/connection-errors#the-endpoint-id-is-not-specified )
db_endpoint=

##### Sentry PHP (server-side) #####
# sentry_php_dsn: Optional, Sentry PHP DSN
sentry_php_dsn=
# sentry_php_env: Optional, Sentry PHP environment 
sentry_php_env=

##### Deployment #####
# web_http_port: Optional, port where to expose the web application on HTTP when deployed with Docker Compose in development and production
web_http_port=80
# web_https_port: Optional, port where to expose the web application on HTTPS when deployed with Docker Compose in production
web_https_port=443
# pgadmin_port: Optional, port where to expose pgAdmin when deployed with Docker Compose in development
pgadmin_port=8000
# swagger_port: Optional, port where to expose Swagger UI when deployed with Docker Compose in development
swagger_port=8001
# allowed_origins: Optional, list of allowed origins for CORS
allowed_origins=["http://localhost:8001"]
# framework_image_tag: Optional, version of OSM-Wikidata Map Framework to use when deployed with Docker Compose in production (one of the image tags in https://gitlab.com/openetymologymap/osm-wikidata-map-framework/container_registry/3939048 )
framework_image_tag=latest

####################################
####                            ####
####       Apache Airflow       ####
####                            ####
####################################

##### Airflow settings, used for DB initialization, unnecessary for a web-only instance #####
AIRFLOW_IMAGE_NAME=registry.gitlab.com/openetymologymap/osm-wikidata-map-framework/airflow
AIRFLOW_PROJ_DIR=./airflow
AIRFLOW_UID=50000

# global_map_threshold: Optional, minimum number of elements a map area must contain to be shown in the global map
# A lower value will result in a more detailed global map, but will also increase the size of the size of the global map file
global_map_threshold=10
