{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.6.0",
    "generators": {
      "overpass": {
        "inputSpec": "./openapi/overpass.yaml",
        "output": "./src/generated/overpass",
        "generatorName": "typescript-fetch",
        "additionalProperties": {
          "modelPropertyNaming": "original"
        }
      },
      "owmf": {
        "glob": "./openapi/owmf.yaml",
        "output": "./src/generated/owmf",
        "generatorName": "typescript-fetch",
        "additionalProperties": {
          "modelPropertyNaming": "original"
        }
      },
      "sparql": {
        "glob": "./openapi/sparql.yaml",
        "output": "./src/generated/sparql",
        "generatorName": "typescript-fetch",
        "additionalProperties": {
          "modelPropertyNaming": "original"
        }
      }
    }
  }
}