{
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "name": "openetymologymap/osm-wikidata-map-framework",
    "description": "OSM-Wikidata Map Framework - Framework for creating interactive maps that shows details about map elements based on OpenStreetMap and Wikidata",
    "type": "project",
    "homepage": "https://gitlab.com/openetymologymap/osm-wikidata-map-framework",
    "license": "GPL-3.0",
    "keywords": [
        "OpenStreetMap",
        "Wikidata",
        "map",
        "mappa",
        "karte",
        "open data",
        "linked data",
        "structured data"
    ],
    "authors": [
        {
            "name": "Daniele Santini",
            "homepage": "https://www.dsantini.it"
        }
    ],
    "funding": [
        {
            "type": "paypal",
            "url": "https://paypal.me/dsantiniit"
        }
    ],
    "require": {
        "php": "^8.0",
        "sentry/sdk": "^3.1"
    },
    "require-dev": {
        "vimeo/psalm": "^5.6",
        "vlucas/phpdotenv": "^5.5"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
